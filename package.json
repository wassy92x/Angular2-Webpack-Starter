{
  "name": "angular2-webpack-starter",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "start": "start http://localhost:3400/ & webpack-dev-server --inline --progress --port 3400",
    "test": "karma start",
    "lint": "tslint \"src/**/*.ts\"",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "add": "node node_modules/angularhelperscripts/src/add.js"
  },
  "license": "MIT",
