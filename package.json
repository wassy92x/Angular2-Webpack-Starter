    "@angular/http": "~2.4.9",
    "@angular/platform-browser": "~2.4.9",
    "@angular/platform-browser-dynamic": "~2.4.9",
    "@angular/router": "~3.4.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.35",
    "@types/node": "^7.0.5",
    "angular2-template-loader": "0.6.0",
    "angularhelperscripts": "git://github.com/wassy92x/angular_helper_scripts.git#0.2.1",
    "argparse": "^1.0.9",
